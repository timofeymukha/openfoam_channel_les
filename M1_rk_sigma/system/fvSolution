FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|pCorr)" 
    {
      solver     GAMG;
      tolerance 5e-06;
      relTol     0.001;
      smoother   DIC;
      cacheAgglomeration true;
      nCellsInCoarsestLevel 10;
      agglomerator faceAreaPair;
      mergeLevels   1;
    }

    "(p|pCorr)Final"
    {
      $p 
      tolerance 5e-06;
      relTol     0;
    }

    U 
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-07;
        relTol          0.01;
    }

    UFinal
    {
        $U;
        tolerance       1e-07;
        relTol          0;
    }
}

RungeKutta
{
    scheme          DIRK2;
    nOuter          1;
    nInner          2;
    pnPredCoef      1;
    pRefCell        0;
    pRefValue       0;
}

